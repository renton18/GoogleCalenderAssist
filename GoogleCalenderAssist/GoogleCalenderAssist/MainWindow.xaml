            <Button Content="同期" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Click="Button_Click_1" RenderTransformOrigin="0.293,5.333" Canvas.Left="299" Canvas.Top="141"/>
            <Label Content="GoogleCalenderからデータを取得します&#xD;&#xA;(上記4項目を設定入力後ボタンを押して下さい）" HorizontalAlignment="Left" Height="42" VerticalAlignment="Top" Width="292" Canvas.Left="10" Canvas.Top="129"/>

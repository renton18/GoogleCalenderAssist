        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="GoogleCalenderAssist.MainWindow"
        Title="MainWindow" Height="314.775" Width="429.45">
    <Grid Margin="0,0,2,-20" Height="283" VerticalAlignment="Top">
        <GroupBox Header="" HorizontalAlignment="Left" Margin="9,10,0,0" VerticalAlignment="Top" Height="57" Width="393">
            <Label Content="2014-2020年のEXCELファイルを作成します" HorizontalAlignment="Left" Height="29" VerticalAlignment="Top" Width="445"/>
        </GroupBox>
        <Button Content="作成" HorizontalAlignment="Left" Margin="309,32,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

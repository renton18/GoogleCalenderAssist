            <Label Content="パスワード" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="49" Canvas.Top="101"/>
            <TextBox x:Name="tbID" Height="23" Canvas.Left="156" TextWrapping="Wrap" Canvas.Top="75" Width="218" BorderBrush="#FF00050A"/>
            <TextBox x:Name="tbPASS" Height="23" Canvas.Left="156" TextWrapping="Wrap" Canvas.Top="103" Width="218" BorderBrush="#FF00050A"/>
            <Label Content="カレンダー１ID" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="28" Canvas.Top="16"/>
            <TextBox x:Name="tbCal1" Height="23" Canvas.Left="156" TextWrapping="Wrap" Canvas.Top="19" Width="218" BorderBrush="#FF00050A"/>
            <Label Content="カレンダー２ID" HorizontalAlignment="Left" VerticalAlignment="Top" Canvas.Left="28" Canvas.Top="45"/>
            <TextBox x:Name="tbCal2" Height="23" Canvas.Left="156" TextWrapping="Wrap" Canvas.Top="47" Width="218" BorderBrush="#FF00050A"/>
        </Canvas>

    </Grid>
</Window>
